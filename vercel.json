{
  "version": 2,
  "builds": [
    {
      "src": "ui/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "api/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.ts"
    },
    {
      "src": "/(.*)",
      "dest": "ui/$1"
    }
  ]
}
