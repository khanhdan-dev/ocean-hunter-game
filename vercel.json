{
  "version": 2,
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "builds": [
    { "src": "ui/package.json", "use": "@vercel/next" },
    { "src": "api/dist/main.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/dist/main.js" },
    { "src": "/(.*)", "dest": "ui/$1" }
  ]
}
