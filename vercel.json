{
  "version": 2,
  "builds": [
    {
      "src": "api/src/main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["api/dist/**"]
      }
    },
    {
      "src": "ui/next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/dist/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "ui/$1"
    }
  ]
}
